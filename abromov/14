#include <bits/stdc++.h>
#include <cmath>
using namespace std;
 int main ()
{ 
    int m1,m2,F,r;
    double G;
    cout << "введите массу первого" << "введите массу второго" << "введите расстояние";
    cin >> m1 >> m2 >> r;
    G=6,67 * (pow (10.0,-11.0));
    F=G * (m1 * m2) / r * r;
    cout << F << " Сила притяжения";
    return 0;
}
