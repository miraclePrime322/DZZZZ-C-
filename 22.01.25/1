#include <bits/stdc++.h>
using namespace std;

void task1 (int a, int b, int c )
{
    if (a>=0)
    {
            cout << a*a;
    }
    if (b>=0)
    {
            cout << b*b;
    }
    if (c>=0)
    {
            cout << c*c;
    }
}
int main() {
int* size3 = new int [3]{1,-2,3};
task1(size3[0],size3[1],size3[2]);

}
